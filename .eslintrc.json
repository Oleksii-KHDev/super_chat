{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-essential",
        "prettier",
        "plugin:node/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "vue",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error"],
        "linebreak-style": 0,
        "no-console": "off",
        "no-tabs": "off",
        "indent": "off",
        "no-mixed-spaces-and-tabs": "off",
        "no-underscore-dangle": ["error"]
    }
}
