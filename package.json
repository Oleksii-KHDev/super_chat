{
  "name": "super_chat",
  "version": "1.0.0",
  "description": "Test task for dZENcode. Chat using Node.js, Vue.js, Socket.io, Express.js, Prisma",
  "main": "/server/server.js",
  "scripts": {
    "start": "node --trace-warnings -r dotenv/config server/server.js dotenv_config_path=.env",
    "dev": "npx nodemon -e ts --ignore 'node_modules/**/node_modules' --exec \"npm run build && node --trace-warnings -r dotenv/config dist/super-chat.js dotenv_config_path=./.env\"",
    "pretty": "prettier --write --ignore-unknown **/*.ts",
    "lint": "eslint --ext .ts **/*",
    "lint:fix": "eslint --fix --color --ext .ts **/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "build": "npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oleksii-KHDev/super_chat.git"
  },
  "keywords": [
    "dZENcode",
    "test",
    "task",
    "chat",
    "Vue.js",
    "Node.js",
    "Socket.io",
    "Express.js"
  ],
  "author": {
    "name": "Oleksii Zolotukhin",
    "email": "alexeyzolotuhin@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Oleksii-KHDev/super_chat/issues"
  },
  "homepage": "https://github.com/Oleksii-KHDev/super_chat#readme",
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.21",
    "prettier": "2.8.4",
    "prisma": "^4.11.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-unknown",
      "eslint --fix --color"
    ]
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@types/http-errors": "^2.0.1",
    "chalk": "^5.2.0",
    "dedent": "^0.7.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "socket.io": "^4.6.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "type": "module"
}
